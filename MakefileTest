CC=gcc
FLAGS=-g
FILES=CloseList.c test.c 
OBJ=CloseList.o test.o 
OUT=a.out

all: build

build: $(OBJ)
	$(CC) $(FLAGS) $(OBJ)

CloseList.o: CloseList.c CloseList.h
	$(CC) $(FLAGS) -c  CloseList.c

test.o: test.c Report.h CloseList.h
	$(CC) $(FLAGS) -c  test.c

clean:
	rm -rf $(OBJ) $(OUT)

rebuild: clean build
